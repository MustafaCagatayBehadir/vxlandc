submodule cisco-dc-common {
    belongs-to cisco-dc {
        prefix "cisco-dc";
    }

    yang-version 1.1;

    import tailf-common {
        prefix tailf;
    }
    import tailf-ncs {
        prefix ncs;
    }

    revision 2022-01-17 {
        description
          "Initial Revision.";
    }

    typedef node-port {
        type string {
            pattern
              '[1-9]/[1-9][0-9]{0,1}';
        }
    }
}